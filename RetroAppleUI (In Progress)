  local sideWin = {}

  function sideWin:CreateTab(text, name)

    _G.tab = Instance.new("TextButton")
    local UICorner_tab = Instance.new("UICorner")

    _G.tab.Name = tostring(name)
    _G.tab.Parent = _G.TabContainer
    _G.tab.BackgroundColor3 = Color3.fromRGB(62, 64, 82)
    _G.tab.Size = UDim2.new(0, 149, 0, 42)
    _G.tab.Font = Enum.Font.SourceSansSemibold
    _G.tab.Text = text
    _G.tab.TextColor3 = Color3.fromRGB(229, 229, 229)
    _G.tab.TextSize = 15.000
    _G.tab.TextWrapped = true
    _G.tab.ZIndex = 2

    UICorner_tab.CornerRadius = UDim.new(0.0199999996, 8)
    UICorner_tab.Parent = _G.tab

    -- Windows -------------------------------------------

    local window = Instance.new("Frame")
    local UIPadding_window = Instance.new("UIPadding")
    local UIListLayout = Instance.new("UIListLayout")
    local UIPadding = Instance.new("UIPadding")
    _G.TextKeyContainer = Instance.new("Frame")

    -- window.Name = tostring(name)
    -- window.Parent = _G.WinContainer
    -- window.BackgroundColor3 = Color3.fromRGB(45, 46, 59)
    -- window.BackgroundTransparency = 1.000
    -- window.BorderSizePixel = 0
    -- window.Position = UDim2.new(0, 178, 0, 20)
    -- window.Size = UDim2.new(0, 306, 0, 378)

    -- UIPadding_window.Parent = _G.window
    -- UIPadding_window.PaddingLeft = UDim.new(0, 8)
    -- UIPadding_window.PaddingTop = UDim.new(0, 8)

    _G.TextKeyContainer.Name = tostring(name)
    _G.TextKeyContainer.Parent = _G.WinContainer
    _G.TextKeyContainer.BackgroundColor3 = Color3.fromRGB(45, 46, 59)
    _G.TextKeyContainer.BorderSizePixel = 0
    _G.TextKeyContainer.ClipsDescendants = true
    _G.TextKeyContainer.Position = UDim2.new(0,-3,0,0)
    _G.TextKeyContainer.Size = UDim2.new(0, 308, 0, 378)

    UIListLayout.Parent = _G.TextKeyContainer
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding = UDim.new(0, 15)

    UIPadding.Parent = _G.TextKeyContainer
    UIPadding.PaddingLeft = UDim.new(0, 50)
    UIPadding.PaddingTop = UDim.new(0, 10)

    for i, v in pairs(_G.TabContainer:GetChildren()) do
      print(_G.TabContainer)
      if v.ClassName == "TextButton" then
          v.MouseButton1Click:Connect(function()
              for i, v2 in pairs(_G.WinContainer:GetChildren()) do
                  if v2.Name ~= v.Name then
                    v2.Visible = false
                      print("Off")
                  else
                    v2.Visible = true
                      print("On")
                  end
              end
          end)
      end
    end
  end


  return sideWin
end

